	<!-- Here is a utility library called "waypoints", a way to trigger a function when you scroll to an element. -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/2.0.3/waypoints.min.js"></script>

	<!-- Here is the countup library -->
	<script src="https://www.jqueryscript.net/demo/Animating-Numbers-Counting-Up-with-jQuery-Counter-Up-Plugin/jquery.counterup.min.js"></script> 
	<!-- Here is the countup JavaScript to configure your counter -->
	<script>
		jQuery(document).ready(function( $ ) {
			$('.counter').counterUp({
				time: 2800 // milliseconds
			});
		});
	</script>
